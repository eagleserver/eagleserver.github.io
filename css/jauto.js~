$("#slideshow-inner > div:gt(0)").hide();

setInterval(function() { 
  $('#slideshow-inner > div:first')
    .fadeOut(20)
    .next()
    .fadeIn(20)
    .end()
    .appendTo('#slideshow-inner');
},  30);
